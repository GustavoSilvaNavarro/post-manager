{
  "cSpell.words": [
    "codepaths",
    "daos",
    "DATABASENAME",
    "hstore",
    "prestart",
    "reduxjs",
    "tailwindcss",
    "toastify",
    "tsbuildinfo"
  ],
  "eslint.workingDirectories": [
    "./backend",
    "./frontend"
  ],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true,
  },
  "files.associations": {
    "*.css": "tailwindcss"
  }
}
